dist.shape <- function(x)
{
n <- length(x)
result <- c(NA,NA)
if (n >= 2)
{
z <- (x-mean(x))/sd(x)
skew <- sum(z^3)/(n-1)
kurt <- sum(z^4)/(n-1)
result <- c(skew,kurt)
}
return(result)
}
